doctype html
html
    head
        title=title
        link(rel="icon" type="image/x-icon" href="media/favicon.ico")
        block head
            each cssFile in cssFiles
                link(type="text/css" rel="stylesheet" href=cssFile)
    body
        block content
        include nav/nav
        block scripts
            each jsFile in jsFiles
                script(type="text/javascript" src=jsFile)